# dh doctor shows uv and Java in output
exec dh doctor
stdout 'uv'
stdout 'Java'

# dh doctor --json outputs valid JSON with required fields
exec dh doctor --json
stdout '"healthy"'
stdout '"checks"'
stdout '"name"'
stdout '"status"'
stdout '"detail"'

# dh doctor --no-color produces no ANSI escape sequences
exec dh doctor --no-color
! stdout '\x1b\['

# dh doctor --help shows --fix flag
exec dh doctor --help
stdout '\-\-fix'
